    "n=int(input())\n",
    "d=[]\n",
    "for i in range(n):\n",
    "    x,y=map(int, input().split())\n",
    "    d.append([x,y,1])\n",
    "for i in range(n):\n",
    "    for j in range(n):\n",
    "        if i==j:\n",
    "            continue\n",
    "        if d[i][0]<d[j][0] and d[i][1]<d[j][1]:\n",
    "            d[i][2]+=1\n",
    "for i in range(n):\n",
    "    print(d[i][2])"
   ]
