    "def find(a):\n",
    "    b=str(a)\n",
    "    for i in range(len(b)):\n",
    "        a=a+int(b[i])\n",
    "    return a\n",
    "\n",
    "n=int(input())\n",
    "i=1\n",
    "while find(i)!=n and i<=1000000:\n",
    "    find(i)\n",
    "    i+=1\n",
    "print(i if find(i)==n else 0)"
   ]
