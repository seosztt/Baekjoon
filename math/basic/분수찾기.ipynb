   "source": [
    "n=int(input())\n",
    "i=0\n",
    "while n>i*(i+1)/2:\n",
    "    i+=1\n",
    "k=n-int(i*(i-1)/2)\n",
    "if i%2==0:\n",
    "    print('{}/{}'.format(k,i-k+1))\n",
    "else:\n",
    "    print('{}/{}'.format(i-k+1,k))"
   ]
  }
