   "source": [
    "t=int(input())\n",
    "import math\n",
    "d=input().split()\n",
    "for i in range(t):\n",
    "    d[i]=int(d[i])\n",
    "for i in range(t):\n",
    "    if d[i]==1:\n",
    "        d[i]=0\n",
    "    else:\n",
    "        for j in range(2, int(math.sqrt(d[i])+1)):\n",
    "            if d[i]%j==0:\n",
    "                d[i]=0\n",
    "print(t-d.count(0))"
   ]
  }
