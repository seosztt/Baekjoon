   "source": [
    "t=int(input())\n",
    "for i in range(t):\n",
    "    a,b = map(int, input().split())\n",
    "    n=int(b-a)\n",
    "    i=0\n",
    "    while n>i*(i+1):\n",
    "        i+=1\n",
    "    print(2*i-1 if i*i-n>=0 else 2*i)"
   ]
  }
